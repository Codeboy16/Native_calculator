      },
      "package": "com.codeboy16.Calculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "3123c97c-a87c-4287-a7cc-f5e28018f272"
